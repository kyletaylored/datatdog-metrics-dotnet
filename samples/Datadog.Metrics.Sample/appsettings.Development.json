{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.Hosting.Lifetime": "Information",
      "Datadog.Metrics": "Debug"
    }
  },
  "DatadogMetrics": {
    // Override settings for Development environment
    // You can set your API key here (but user secrets is more secure)
    "ApiKey": "",
    "FlushIntervalSeconds": 5,
    "DefaultTags": [
      "env:development",
      "service:datadog-metrics-sample",
      "version:1.0.0-dev",
      "developer:local"
    ]
  }
}
